#!python
import argparse
from joshq import joshq



if __name__ == "__main__":
    parser = argparse.ArgumentParser()
    parser.add_argument("--files", nargs="+")
    args = parser.parse_args()

    for file in args.files:
        if file.endswith('.py'):
            with open(file, 'r') as infile:
                joshq.parse(infile.read())

