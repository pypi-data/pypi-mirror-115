FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


defaultFieldValues
(
    volScalarFieldValue permeability 1e-15
);

regions
(
    zoneToCell
    {
        name "layer2A";
        fieldValues
        (
            volScalarFieldValue permeability 4e-14
        );
    }
    zoneToCell
    {
        name "layer2B";
        fieldValues
        (
            volScalarFieldValue permeability 1e-15
        );
    }
);

