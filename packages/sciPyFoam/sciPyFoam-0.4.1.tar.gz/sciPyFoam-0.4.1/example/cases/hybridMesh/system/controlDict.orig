FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application HydrothermalSinglePhaseDarcyFoam;

startFrom latestTime;

startTime 0;

stopAt endTime;

endTime 5266512000;

deltaT 864000;

adjustTimeStep yes;
maxCo           0.8; 
maxDeltaT       157680000; 

writeInterval 15768000;

writeControl adjustableRunTime;

purgeWrite 1;

writeFormat ascii;

writePrecision 8;

writeCompression off;

timeFormat general;

timePrecision 14;

runTimeModifiable true;

libs 
( 
    "libHydrothermalBoundaryConditions.so"
    "libHydroThermoPhysicalModels.so"
);
