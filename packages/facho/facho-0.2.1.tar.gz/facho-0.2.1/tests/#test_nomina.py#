"""Tests for `facho` package."""

import pytest

import facho.model
import facho.model.fields as fields


def test_raiz():
    class Nomina(model.Model):
        __name__ = 'Nomina'

    nomina = Nomina()
    assert "<Nomina/>" == nomina.to_xml

