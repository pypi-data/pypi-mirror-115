__version__ = 50

import ob
ob.wd = "/var/lib/botd/"

from trm import CLI, Runtime, wrap
k = Runtime()
k.cfg.name = "botd"
k.cfg.version = __version__

import bot.all

def main():
    k.boot()
    c = CLI()
    return c.cmd(k.cfg.otxt)

wrap(main)
